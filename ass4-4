using System;

namespace Ass4
{
    class Customer
    {
        int Customerid;
        string Name;
        int Age;
        char Phone;
        string City;

        public Customer()
        {
            Customer = new Customer();
        }
        
        public Customer(int Customerid, string Name, int Age, char Phone, string City)
        {
            this.Customerid = Customerid;
            this.Name = Name;
            this.Age = Age;
            this.Phone = Phone;
            this.City =  City;
        }